                    <div style="width: 40vw;padding: 2rem;">
                        <h6 style="color: red;"><%=locals.msg %></h6> 
                        <div class="mb-3">
                            <label class="form-label" for="productname"> Name</label>
                            <input id="productname" name="name" value="<%=Data.name %>" type="text" class="form-control" required>
                        </div>
        
                        <div class="mb-0">
                            <label class="form-label"  for="productdesc"> Description</label>
                            <textarea class="form-control" name="description" id="productdesc" rows="4" required ><%=Data.description %></textarea>
                        </div>
                        <br>
                     <div class="justify-content-center d-flex ">
                       <button class="btn btn-dark "> <i class=" bx bx-file me-1"></i> Edit</button> 
                    </div> 
                    <input type="hidden" name="cid" value="<%= Data._id %>">
                    </div>
            </form>
            </div>
        
        </div>
    </div> 
       
    <script src="https://cdn.jsdelivr.net/npm/sweetalert2@11"></script>
    <script>
        const form = document.getElementById(`myForm`);
        const inp = form.querySelectorAll('input');
        const textArea=document.querySelector('textarea');

        form.addEventListener('submit',(data)=>{
            let flag=true;
            inp.forEach(elem=>{
                if(elem.value.trim()==''|| textArea.value.trim()==''){
                    flag=false
                }
            })
            if(flag !==true){
                Swal.fire({
                    icon: "error",
                    title: "Oops...",
                    text: "plaese fill the all inputs!",
                   
                });
                data.preventDefault()
            }
        })
    </script>
    

</body>
</html>
